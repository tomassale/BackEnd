<%- include('./partials/encabezado.ejs') %>
<form action="/nombre" method="post" onsubmit="return post(this)" style="width: 500px; margin: auto; margin-top: 100px; font-size: 30px; background-color: rgb(236, 240, 236)">
  <div class="form-group">
    <label for="usuario">Ingrese su nombre</label>
    <input type="text" class="form-control" name="usuario" aria-describedby="nombreUsuario">
    <small class="form-text text-muted">Su nombre se va a mostrar únicamente en su sesión</small>
  </div>
  <button type="submit" class="btn btn-primary">Ingresar</button>
</form>
